<?xml version="1.0" encoding="UTF-8"?>
<!--<!DOCTYPE Models SYSTEM "./dtd/Model.dtd">-->
<!--元模型定义了所有的模型， 并约束所有的模型-->
<MetaModel>
    <Version>0.0.1</Version>
    <Models>
        <Model import="/models/AbstractModel.xml">
            <Id>AbstractModel</Id>
            <Name>抽象模型</Name>
            <Desc>用于定义模型的通用部分</Desc>
        </Model>

        <Model extends="AbstractModel" import="/models/MetaFieldModel.xml">
            <Id>MetaFieldModel</Id>
            <Name>字段模型</Name>
            <Desc>用于定义字段的基础</Desc>
        </Model>

        <Model extends="AbstractModel" import="/models/MetaPageModel.xml">
            <Id>MetaPageModel</Id>
            <Name>页面模型</Name>
            <Desc>用于定义页面的内容</Desc>
        </Model>

        <Model extends="AbstractModel" import="/models/MetaEntityModel.xml">
            <Id>MetaEntityModel</Id>
            <Name>数据模型</Name>
            <Desc>用于数据层面的信息定义</Desc>
        </Model>

    </Models>
</MetaModel>