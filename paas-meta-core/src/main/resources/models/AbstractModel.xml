<?xml version="1.0" encoding="UTF-8"?>
<models>
    <model mid="AbstractModel">
        <properties>
            <property>
                <id>id</id>
                <name>ID</name>
                <dataType>Long</dataType>
                <desc>技术上的唯一属性</desc>
            </property>

            <property>
                <id>key</id>
                <name>标识</name>
                <dataType>String</dataType>
                <desc>可见的唯一属性</desc>
            </property>

            <property>
                <id>name</id>
                <name>名称</name>
                <dataType>String</dataType>
                <desc>中文名称</desc>
            </property>

            <property>
                <id>desc</id>
                <name>描述</name>
                <dataType>String</dataType>
                <desc>关于属性的描述</desc>
            </property>

            <property>
                <id>version</id>
                <name>版本</name>
                <dataType>String</dataType>
                <desc>版本号</desc>
            </property>

            <property>
                <id>type</id>
                <name>类型</name>
                <dataType>Enum</dataType>
                <desc>模型类型</desc>
            </property>

            <property>
                <id>status</id>
                <name>状态</name>
                <dataType>Integer</dataType>
                <desc>描述元素的状态</desc>
            </property>

            <property>
                <id>enable</id>
                <name>启用</name>
                <dataType>Boolean</dataType>
                <desc>启用状态， null / 1：启用， 0 ：禁用</desc>
            </property>

            <property>
                <id>visible</id>
                <name>可见</name>
                <dataType>Boolean</dataType>
                <desc>可见性， null / 1：可见， 0 ：不可见</desc>
            </property>
        </properties>

        <modelProps>
            <modelProp>
                <ref>id</ref>
                <ref>key</ref>
                <ref>name</ref>
                <ref>desc</ref>
                <ref>version</ref>
                <ref>type</ref>
                <ref>status</ref>
                <ref>enable</ref>
                <ref>visible</ref>
            </modelProp>
        </modelProps>
    </model>
</models>
